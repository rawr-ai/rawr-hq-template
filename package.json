{
  "name": "rawr-hq-template",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.7",
  "workspaces": [
    "apps/*",
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "pretest:vitest": "turbo run build",
    "test:vitest": "vitest run",
    "test": "bun run test:vitest",
    "rawr": "bun run --cwd apps/cli rawr"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitest/ui": "^4.0.18",
    "turbo": "^2.7.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
