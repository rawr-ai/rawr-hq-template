{
  "name": "@rawr/cli",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.7",
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.27",
    "@oclif/plugin-plugins": "^5.4.36",
    "@rawr/agent-sync": "workspace:*",
    "@rawr/coordination": "workspace:*",
    "@rawr/control-plane": "workspace:*",
    "@rawr/core": "workspace:*",
    "@rawr/journal": "workspace:*",
    "@rawr/plugin-plugins": "workspace:*",
    "@rawr/state": "workspace:*",
    "@rawr/security": "workspace:*",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "@types/node": "^22.14.0",
    "typescript": "^5.9.3"
  },
  "oclif": {
    "bin": "rawr",
    "commands": "./src/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@rawr/plugin-plugins"
    ],
    "typescript": {
      "commands": "./src/commands"
    }
  },
  "scripts": {
    "rawr": "bun src/index.ts",
    "dev:cli": "bun src/index.ts",
    "build": "bunx tsc -p tsconfig.json",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "lint": "eslint --max-warnings 0 --no-error-on-unmatched-pattern \"**/*.{js,jsx,ts,tsx}\""
  },
  "bin": {
    "rawr": "./bin/run.js"
  }
}
