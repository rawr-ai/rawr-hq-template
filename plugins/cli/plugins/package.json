{
  "name": "@rawr/plugin-plugins",
  "version": "0.1.0",
  "private": true,
  "description": "Unified plugins command surface for sync, runtime web plugins, and plugin scaffolding",
  "license": "MIT",
  "type": "module",
  "packageManager": "bun@1.3.7",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@rawr/core": "workspace:*",
    "@rawr/agent-sync": "workspace:*",
    "@rawr/control-plane": "workspace:*",
    "@rawr/security": "workspace:*",
    "@rawr/state": "workspace:*"
  },
  "oclif": {
    "commands": "./src/commands",
    "topicSeparator": " ",
    "typescript": {
      "commands": "./src/commands"
    }
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bunx tsc -p tsconfig.json",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "test": "vitest run --project plugin-plugins",
    "lint": "eslint --max-warnings 0 --no-error-on-unmatched-pattern \"**/*.{js,jsx,ts,tsx}\""
  },
  "rawr": {
    "templateRole": "example",
    "kind": "toolkit",
    "channel": "A",
    "publishTier": "blocked"
  }
}
