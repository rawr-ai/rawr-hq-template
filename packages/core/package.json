    "@rawr/coordination": "workspace:*",
    "@rawr/state": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bunx tsc -p tsconfig.json",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  }
