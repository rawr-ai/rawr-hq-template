{
  "name": "@rawr/core",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.7",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./orpc": {
      "types": "./src/orpc/index.ts",
      "default": "./src/orpc/index.ts"
    }
