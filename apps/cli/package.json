{
  "name": "@rawr/cli",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.7",
  "dependencies": {
    "@oclif/core": "^4.2.10",
    "@oclif/plugin-help": "^6.2.27",
    "@oclif/plugin-plugins": "^5.4.36",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.9.3"
  },
  "oclif": {
    "bin": "rawr",
    "commands": "./src/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "typescript": { "commands": "./src/commands" }
  },
  "scripts": {
    "rawr": "bun src/index.ts",
    "dev:cli": "bun src/index.ts",
    "build": "bunx tsc -p tsconfig.json",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  }
}
