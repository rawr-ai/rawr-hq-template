    "@rawr/core": "workspace:*",
    "@rawr/ui-sdk": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@playwright/test": "^1.56.1",
    "autoprefixer": "^10.4.21",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
