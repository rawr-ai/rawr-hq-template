{
  "name": "@rawr/plugin-hello",
  "version": "0.1.0",
  "private": true,
  "description": "RAWR HQ-Template sample oclif plugin",
  "license": "MIT",
  "type": "module",
  "packageManager": "bun@1.3.7",
  "files": [
    "dist",
    "README.md",
    "package.json"
