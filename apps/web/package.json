{
  "name": "@rawr/web",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.7",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json --noEmit && vite build",
    "preview": "vite preview",
